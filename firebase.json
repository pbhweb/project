{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/__/auth/handler",
        "destination": "/__/auth/handler",
        "type": 301
      },
      {
        "source": "/auth/**",
        "function": "app"
      },
      {
        "source": "/__/**",
        "destination": "project/desktop.html"
      },
      {
        "source": "**",
        "destination": "project/desktop.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "*/.",
      "/node_modules/"
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      {
        "source": "/__/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ],
    "redirects": []
  }
}